{
  "name": "retail-management-backend",
  "version": "1.0.0",
  "description": "Fastify backend for retail management system",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "postinstall": "prisma generate && tsc",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:seed": "tsx prisma/seed.ts",
    "prisma:reset": "prisma migrate reset --force && pnpm prisma:seed",
    "prisma:reset-data": "tsx reset-db.ts",
    "test": "jest"
  },
  "keywords": [
    "fastify",
    "retail",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/cookie": "^9.4.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/jwt": "^7.1.1",
    "@fastify/multipart": "^8.0.0",
    "@fastify/session": "^10.6.0",
    "@fastify/websocket": "^7.0.0",
    "@prisma/client": "^6.2.0",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "bcrypt": "^6.0.0",
    "dotenv": "^17.2.1",
    "fastify": "^4.27.0",
    "socket.io": "^4.8.1",
    "uuid": "^11.1.0",
    "ws": "^8.18.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-typescript": "^7.27.1",
    "@types/bcrypt": "^6.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.0.0",
    "@types/supertest": "^6.0.3",
    "babel-jest": "^30.0.5",
    "jest": "^30.0.5",
    "prisma": "^6.2.0",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.0.0"
  }
}
